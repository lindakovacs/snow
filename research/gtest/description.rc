#include "winres.h"
#include <build/version.h>

 VS_VERSION_INFO VERSIONINFO
 FILEVERSION BUILD_VERSION_MAJOR,BUILD_VERSION_MINOR,BUILD_VERSION_PATCH,BUILD_NUMBER
 PRODUCTVERSION BUILD_VERSION_MAJOR,BUILD_VERSION_MINOR,BUILD_VERSION_PATCH,BUILD_NUMBER
 FILEFLAGSMASK 0x17L
